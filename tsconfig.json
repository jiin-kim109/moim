{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@app/*": ["./src/app/*"],
      "@components/*": ["./src/components/*"],
      "@lib/*": ["./src/lib/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@assets/*": ["./assets/*"],
    }
  },
  "include": [
    "nativewind-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
}
